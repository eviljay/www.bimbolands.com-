	$(window).resize();


	// RADIO BUTTON REVEAL //
	$('input.b-special-reveal-trigger[type=radio] + label').click(function(){

		// Defining variables
		var action = $(this).prev().attr('data-special-reveal-action');
		var target = $(this).prev().attr('data-special-reveal-target');
		var timer = 300;
		
		// Checks if it's revealing or not and adds class when visible, only hides if there's class
		if(action == 'true') {
			if($(target).hasClass('active')) {
				return false;
			} else {
				$(target).addClass('active');
				$(target).slideDown(timer);
			}
		} else {
			if($(target).hasClass('active')) {
				$(target).removeClass('active');
				$(target).slideUp(timer);
			} else {
				return false;
			}
		}

	});


	// JUMP SPECIAL //
	$('.b-button-special-jump').click(function(){

		// Defining variables
		var target = $(this).attr('data-special-jump');
		var timer = 300;

		// Scrolls to top and exits
		$('html, body').animate({scrollTop: $(target).offset().top}, timer);
		return false;

	});


	// CLEAR TEXTAREA //
	$('.b-button-special-clear').click(function(){

		// Defining variables
		var target = $(this).attr('data-special-clear-target');
		var placeholder = $(this).attr('data-special-clear-placeholder');

		// Checks for target existence
		if(typeof target == 'undefined') {
			return false;
		}

		// Checks for placeholder existence
		if(typeof placeholder == 'undefined') {
			placeholder = '';
		}

		// If target is wysihtml5 clears iframe body and adds class .placeholder, else clears .val() of target
		if(target == 'wysihtml5-sandbox') {
			$('iframe.' + target).contents().find('body').text(placeholder).addClass('placeholder');
		} else {
			$('#' + target).val('');
		}
		return false;

	});


